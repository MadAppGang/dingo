package main

import (
	"encoding/json"
	"fmt"
	"os"
)

type Config struct {
	Host string
	Port int
}

func pipeline(path string) (*Config, error) {
	tmp, err := os.ReadFile(path)

	if err != nil {
		return nil, fmt.Errorf("failed to read config: %w", err)
	}
	// dingo:e:1
	var data = tmp
	var cfg Config
	tmp1, err1 := json.Unmarshal(data, &cfg)

	if err1 != nil {
		return nil, fmt.Errorf("failed to parse config: %w", err1)
	}
	// dingo:e:1
	var err = tmp1
	return &cfg, nil
}
