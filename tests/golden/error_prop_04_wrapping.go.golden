package main

import (
	"fmt"
	"os"
)

func readUserConfig(username string) ([]byte, error) {
	path := "/home/" + username + "/config.json"
	tmp, err := os.ReadFile(path)

	if err != nil {
		return nil, fmt.Errorf("failed to read user config: %w", err)
	}
	// dingo:e:1
	var data = tmp
	return data, nil
}
